{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "outDir": "./dist",
    "baseUrl": "./src",
    "rootDir": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false,
    "noImplicitAny": true,
     "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
   "exclude": [
    "node_modules",
     "**/*.test.ts"
  ]
}